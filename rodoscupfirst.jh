entity Championship {
	year Long
}

entity Leg {
	name String
}

relationship OneToMany {
	Championship{leg} to Leg
}

entity YachtCategory {
	name String
}

relationship OneToMany {
	Championship{yachtCategory} to YachtCategory
}

entity RaceResult {
	url	String
}

relationship OneToMany {
	RaceResult{leg} to Leg,
    RaceResult{category} to YachtCategory
}

