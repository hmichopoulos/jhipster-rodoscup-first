entity Championship {
	year Long
}

entity Leg {
	name String
}

relationship OneToMany {
	Championship{leg} to Leg
}

entity YachtCategory {
	name String
}

relationship OneToMany {
	Championship{yachtCategory} to YachtCategory
}

entity Result {
	url	String
}

relationship OneToMany {
	Result{leg} to Leg,
    Result{category} to YachtCategory
}

